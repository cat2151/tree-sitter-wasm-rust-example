{
  "name": "tree-sitter-wasm-rust-example",
  "version": "0.1.0",
  "description": "Cross-platform language processing toolchain example",
  "main": "grammar.js",
  "scripts": {
    "build:parser": "tree-sitter generate && tree-sitter build-wasm",
    "build:wasm": "cd src/rust && wasm-pack build --target web --out-dir ../../src/browser/pkg",
    "build": "npm run build:parser && npm run build:wasm"
  },
  "dependencies": {
    "web-tree-sitter": "^0.20.8"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.20.8"
  }
}
